{
  "name": "educatefy-backend",
  "version": "1.0.0",
  "description": "A web application for online learning.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "npx tsc",
    "check-types": "tsc --noEmit",
    "create-migration": "cd src/db && npx knex migrate:make",
    "list-migrations": "cd src/db && npx knex migrate:list",
    "migrate": "cd src/db && npx knex migrate:latest",
    "update-db-types": "npx tsx ./src/scripts/generateDbTypes.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "knex": "^3.1.0",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "eslint": "^8",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jest": "^27",
    "eslint-plugin-n": "^16",
    "eslint-plugin-prettier": "^5",
    "eslint-plugin-promise": "^6",
    "knex-types": "^0.5.0",
    "prettier": "^3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}

{
  "name": "educatefy-backend",
  "version": "1.0.0",
  "description": "A web application for online learning.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "npx tsc",
    "check-types": "tsc --noEmit",
    "gql-codegen": "graphql-codegen",
    "create-migration": "cd src/db && npx knex migrate:make",
    "list-migrations": "cd src/db && npx knex migrate:list",
    "migrate": "cd src/db && npx knex migrate:latest",
    "update-db-types": "npx tsx ./src/scripts/generateDbTypes.ts",
    "generate-loaders": "npx tsx ./src/scripts/generateLoaders.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.47.3",
    "cors": "^2.8.5",
    "dataloader": "^2.2.3",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dompurify": "^3.2.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "graphql": "^16.9.0",
    "graphql-yoga": "^5.10.4",
    "helmet": "^8.0.0",
    "jsdom": "^25.0.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "maxmind": "^4.3.23",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.0.9",
    "openid-client": "^5.6.5",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.1.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "eslint": "^8",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jest": "^27",
    "eslint-plugin-n": "^16",
    "eslint-plugin-prettier": "^5",
    "eslint-plugin-promise": "^6",
    "knex-types": "^0.5.0",
    "prettier": "^3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
